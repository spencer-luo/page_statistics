{
  "name": "page-stats-tracker",
  "version": "1.0.0",
  "description": "Page view statistics tracker",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./dist",
    "start": "node ./dist/server.js",
    "dev": "ts-node server.ts",
    "watch": "tsc -w",
    "deploy:prepare": "npm run build && node -e \"require('fs').copyFileSync('package.json', 'dist/package.json'); require('fs').copyFileSync('package-lock.json', 'dist/package-lock.json');\"",
    "deploy:server": "cd ./dist && npm install --production"
  },
  "imports": {
    "#config": {
      "default": "./config.js"
    },
    "#logger": {
      "default": "./logger.js"
    },
    "#models/*": {
      "default": "./models/*.js"
    },
    "#routes/*": {
      "default": "./routes/*.js"
    },
    "#counter/*": {
      "default": "./counter/*.js"
    }
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.0",
    "@types/uuid": "^9.0.4",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
